.userImg {
  max-width: 50px;
  max-height: 50px;
  border: 2px solid;
  border-radius: 4px;
  margin-right: 10px;
}

.camperRow{
  margin-top: 1px;
  margin-bottom: 0px;
}

table.table-striped {
  background-color: #f0f0f0;
  color: #333;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  border-collapse: inherit;
  border: 1px solid #351f0b;
}

#tableHeader {
  text-align: center;
}

table, th, td {
  border-top: 1px solid black;
  border-bottom: 1px solid black;
}

table {
  width: 100%;
}

th, td {
  text-align: left;
}